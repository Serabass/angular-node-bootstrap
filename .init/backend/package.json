{
  "name": "express-typescript-jest",
  "version": "1.0.0",
  "description": "Bootstrap Project With Express Typescript and Jest",
  "main": "src/app.js",
  "scripts": {
    "test": "jest",
    "tsc": "tsc",
    "dev": "ts-node-dev --respawn --transpileOnly ./src/app.ts",
    "prod": "tsc && node ./dist/app.js",
    "lint": "tsc --noEmit && eslint --ext .ts,.tsx -c ./.eslintrc.js ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Isisphinx/Express-Typescript-Jest.git"
  },
  "author": "Thibaut Constant",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/Isisphinx/Express-Typescript-Jest/issues"
  },
  "homepage": "https://github.com/Isisphinx/Express-Typescript-Jest#readme",
  "devDependencies": {
    "@types/express": "^4.16.1",
    "@types/jest": "^24.0.12",
    "@types/node": "^11.13.8",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-prettier": "^3.0.1",
    "jest": "^24.7.1",
    "prettier": "^1.17.0",
    "ts-jest": "^24.0.2",
    "ts-node-dev": "^1.0.0-pre.32",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "express": "^4.16.4"
  }
}
